<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>
        {% block header %}

        <a href="{{ path('app_index') }}">
        Accueil
        </a>
        <a href="{{ path('category_index') }}" class="flex d-flex justify-content-end">
        catégories
        </a>
        <a href="{{ path('game_index') }}" class="flex d-flex justify-content-end">
        Les jeux
        </a>
        {% endblock %}



        {% block body %}
        <h1>Le repère du jeu</h1>
        {% endblock %}

    </body>
</html>
